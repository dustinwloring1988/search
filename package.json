{
  "name": "ai-browser-automation",
  "version": "1.0.0",
  "description": "AI-powered browser automation with natural language control using ollama and Playwright",
  "main": "dist/main/main.js",
  "scripts": {
    "start": "electron dist/main/main.js",
    "dev": "tsc && electron .",
    "build": "tsc",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "jest",
    "package": "electron-builder"
  },
  "keywords": [
    "electron",
    "typescript",
    "ollama",
    "playwright",
    "ai",
    "browser-automation"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "electron-log": "^5.0.0",
    "playwright": "^1.40.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
